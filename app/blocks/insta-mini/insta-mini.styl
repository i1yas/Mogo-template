.insta-mini
	&__photos
		clearfix()
	&__photo	
		margin-right 2px
		margin-bottom 2px
		float left
		@media(max-width 540px)
			width 48%
	&__sign
		margin-top 10px
		padding-y 10px
		display inline-block
		font-style italic
		text-decoration none
		color #333
		&:hover
			color $red